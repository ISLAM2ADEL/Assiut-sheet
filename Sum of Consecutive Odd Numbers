#include<iostream>
#include<bits/stdc++.h>
#include<cmath>
#define ll long long
using namespace std ;

int main() {
    ll x,y,z;
    cin>>x;
    for(int i=0;i<x;i++){
        cin>>y>>z;
        ll count=0;
        for(int j=min(y,z)+1;j<=max(y,z)-1;j++){
            if(j%2!=0)
                count+=j;
        }
        cout<<count<<endl;
    }
    return 0;
}
