#include<iostream>
#include<bits/stdc++.h>
#include<cmath>
#define ll long long
using namespace std ;

int main() {
    int x;
    for(int i=1;;i++){
        cin>>x;
        if(x==1999){
            cout<<"Correct"<<endl;
            break;
        }
        else
            cout<<"Wrong"<<endl;
    }
    return 0;
}
