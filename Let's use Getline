#include<iostream>
#include<algorithm>
#include<string.h>
#include <cmath>
#define ll long long
using namespace std;

int main()
{
    string x;
    getline(cin,x);  // string l7d awl msafa enma el getline l7d ema t5ls el klam wtnhy b enter
    ll y=x.length();
    for(ll i=0;i<y;i++){
        if(x[i]!='\\')
            cout<<x[i];
        else
            break;
    }
    return 0;
}
