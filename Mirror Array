#include<iostream>
#include<algorithm>
#include<string.h>
#include <cmath>
#define ll long long
using namespace std;

int main()
{
    ll x,y;
    cin>>x>>y;
    ll a[x][y];
    for(int i=0;i<x;i++)
        for(int j=0;j<y;j++) 
            cin>>a[i][j];
    for(int i=0;i<x;i++){
        for(int j=y-1;j>=0;j--)
            cout<<a[i][j]<<" ";
        cout<<endl;
    }
    return 0;
}
