#include<iostream>
#include<algorithm>
#include<string.h>
#include <cmath>
#define ll long long
using namespace std;

int main()
{
    ll x,z,n=0;
    cin>>x;
    ll y[x];
    for(int i=0;i<x;i++)
        cin>>y[i];
    cin>>z;
    for(int i=0;i<x;i++)
        if(z==y[i]){
            cout<<i<<endl;
            n++;
            break;
        }
    if(n==0)
        cout<<-1<<endl;
    return 0;
}
