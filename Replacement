#include<iostream>
#include<algorithm>
#include<string.h>
#include <cmath>
#define ll long long
using namespace std;

int main()
{
    ll x;
    cin>>x;
    ll y[x];
    for(int i=0;i<x;i++){
        cin>>y[i];
        if(y[i]>0)
            y[i]=1;
        else if(y[i]<0)
            y[i]=2;
        else
            y[i]=0;
    }
    for(int i=0;i<x;i++)
        cout<<y[i]<<" ";
    return 0;
}
