#include<iostream>
#include<bits/stdc++.h>
#define ll long long
using namespace std ;
int main() {
    char x;
    cin>>x;
    int y=(int)x;
    if(y>=65 && y<=90)
        cout<<"ALPHA"<<endl<<"IS CAPITAL"<<endl;
    else if ( y>=97 && y<=122)
        cout<<"ALPHA"<<endl<<"IS SMALL"<<endl;
    else
        cout<<"IS DIGIT"<<endl;
    return 0;
}
