#include<iostream>
#include<bits/stdc++.h>
#define ll long long
using namespace std ;
int main() {
    int x,y;
    char z;
    cin>>x>>z>>y;
    if(z=='>')
        x>y?cout<<"Right":cout<<"Wrong";
    else if(z=='<')
        x<y?cout<<"Right":cout<<"Wrong";
    else if(z=='=')
        x==y?cout<<"Right":cout<<"Wrong";
    return 0;
}
