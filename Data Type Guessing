#include<iostream>
#include<bits/stdc++.h>
#include<cmath>
#define ll long long
using namespace std ;
 
int main() {
    double x,y,z;
    cin>>x>>y>>z;
    double result=(x*y)/z;
    ll xx=result;
    double myres=result-xx;
    if(myres>0){
        cout<<"double"<<endl;
        return 0;
    }
    if (xx<=2147483647)
        cout<<"int"<<endl;
    else
        cout<<"long long"<<endl;
    return 0;
}
