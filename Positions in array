#include<iostream>
#include<algorithm>
#include<string.h>
#include <cmath>
#define ll long long
using namespace std;

int main()
{
    ll x;
    cin>>x;
    ll y[x];
    for(int i=0;i<x;i++)
        cin>>y[i];
    for(int i=0;i<x;i++)
        if(y[i]<=10)
            cout<<"A["<<i<<"] = "<<y[i]<<endl ;
    return 0;
}
