#include<iostream>
#include<bits/stdc++.h>
#include<cmath>
#define ll long long
using namespace std ;

int main() {
    ll x,y;
    cin>>x;
    for(int i=0;i<x;i++){
        cin>>y;
        if(y==0)
            cout<<0;
        else{
                while(y!=0){
                    cout<<y%10<<" ";
                    y/=10;
                }
        }
        cout<<endl;
    }
    return 0;
}
